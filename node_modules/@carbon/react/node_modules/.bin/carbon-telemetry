#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@carbon+telemetry@0.1.0/node_modules/@carbon/telemetry/bin/carbon-telemetry.js" "$@"
else
  exec node  "$basedir/../../../../../../@carbon+telemetry@0.1.0/node_modules/@carbon/telemetry/bin/carbon-telemetry.js" "$@"
fi
