#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/alber/Downloads/Coding/Personal/stopGettingHit/node_modules/.pnpm/node-sass@8.0.0/node_modules/node-sass/bin/node_modules:/mnt/c/Users/alber/Downloads/Coding/Personal/stopGettingHit/node_modules/.pnpm/node-sass@8.0.0/node_modules/node-sass/node_modules:/mnt/c/Users/alber/Downloads/Coding/Personal/stopGettingHit/node_modules/.pnpm/node-sass@8.0.0/node_modules:/mnt/c/Users/alber/Downloads/Coding/Personal/stopGettingHit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/alber/Downloads/Coding/Personal/stopGettingHit/node_modules/.pnpm/node-sass@8.0.0/node_modules/node-sass/bin/node_modules:/mnt/c/Users/alber/Downloads/Coding/Personal/stopGettingHit/node_modules/.pnpm/node-sass@8.0.0/node_modules/node-sass/node_modules:/mnt/c/Users/alber/Downloads/Coding/Personal/stopGettingHit/node_modules/.pnpm/node-sass@8.0.0/node_modules:/mnt/c/Users/alber/Downloads/Coding/Personal/stopGettingHit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/node-sass" "$@"
else
  exec node  "$basedir/../../bin/node-sass" "$@"
fi
